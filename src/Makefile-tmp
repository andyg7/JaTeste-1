TARFILES = Makefile scanner.mll parser.mly ast.ml interpreter.ml myprinter.ml

OBJS = ast.cmo parser.cmo scanner.cmo myprinter.cmo interpreter.cmo

interpreter : $(OBJS)
	ocamlc -o interpreter $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -f interpreter parser.ml parser.mli scanner.ml *.cmo *.cmi

all: clean interpreter
# Generated by ocamldep *.ml *.mli
interpreter.cmo: scanner.cmo parser.cmi ast.cmi 
interpreter.cmx: scanner.cmx parser.cmx ast.cmi 
parser.cmo: ast.cmi parser.cmi 
parser.cmx: ast.cmi parser.cmi 
scanner.cmo: parser.cmi 
scanner.cmx: parser.cmx 
parser.cmi: ast.cmi

